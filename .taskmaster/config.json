{
  "models": {
    "main": {
      "provider": "openrouter",
      "modelId": "deepseek/deepseek-chat-v3-0324:free",
      "maxTokens": 120000,
      "temperature": 0.2
    },
    "research": {
      "provider": "perplexity",
      "modelId": "sonar-pro",
      "maxTokens": 8700,
      "temperature": 0.1
    },
    "fallback": {
      "provider": "openrouter",
      "modelId": "google/gemini-2.0-flash-exp:free",
      "maxTokens": 8192,
      "temperature": 0.1
    }
  },
  "global": {
    "logLevel": "info",
    "debug": false,
    "defaultSubtasks": 3,
    "defaultPriority": "medium",
    "workingTaskGroup": "storybook",
    "projectName": "Taskmaster",
    "defaultFlow": [
      {
        "status": "pending",
        "executor": "agent"
      },
      {
        "status": "in-progress",
        "executor": "agent"
      },
      {
        "status": "review",
        "executor": "agent"
      },
      {
        "status": "review",
        "executor": "human"
      }
    ],
    "ollamaBaseURL": "http://localhost:11434/api",
    "bedrockBaseURL": "https://bedrock.us-east-1.amazonaws.com",
    "currentGroup": "default",
    "azureOpenaiBaseURL": "https://your-endpoint.openai.azure.com/",
    "userId": "1234567890"
  }
}